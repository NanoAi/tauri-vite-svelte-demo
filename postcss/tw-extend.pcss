/* stylelint-disable selector-class-pattern */

.info-text {
  font-weight: 600;
  color: rgb(250 240 240 / var(--tw-text-opacity));
}

.clearfix::after {
  content: '';
  clear: both;
  display: table;
}

.scroll-clip {
  position: relative;
  width: 100%;
  margin-right: -1rem;
  overflow: hidden;
}

.card {
  padding: 1em;
  margin: 1em;
}

.max-w-1\/2 {
  max-width: 50%;
}
